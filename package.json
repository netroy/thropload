{
  "name": "thropload",
  "version": "1.0.0",
  "description": "Throttled http server that streams PUT/POST to /dev/null. for testing http uploads",
  "packageManager": "pnpm@8.6.5",
  "engines": {
    "node": ">=16.9",
    "pnpm": ">=8.6"
  },
  "main": "./dist/index.js",
  "bin": {
    "thropload": "./bin/thropload"
  },
  "dependencies": {
    "debug": "4.3.4",
    "progress": "2.0.3",
    "stream-parser": "0.3.1"
  },
  "devDependencies": {
    "@types/node": "18.16.16",
    "@types/progress": "2.0.5",
    "typescript": "5.1.6"
  },
  "scripts": {
    "build": "tsc",
    "start": "DEBUG=thropload ./bin/thropload"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/netroy/thropload.git"
  },
  "keywords": [
    "throttle",
    "upload"
  ],
  "author": "Aditya <aditya@netroy.in>",
  "license": "MIT"
}
